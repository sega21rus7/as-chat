{
  "name": "as-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "submodules_init": "git submodule init && git submodule update",
    "init": "npm run submodules_init",
    "postinit": "npm run init_client && npm run init_server",
    "build_client": "cd client && npm run build",
    "build_exe": "cd server && node buildExe",
    "init_client": "cd client && npm i",
    "init_server": "npm run submodules_init",
    "postinit_server": "cd server && npm i && cd as-server && npm i",
    "client": "cd client && npm run start",
    "server": "cd server && npm run start",
    "server_prod": "cd server && set NODE_ENV=production&&npm run start_prod",
    "build_container": "docker build -f ./dockerfile -t as-template .",
    "restart_container": "docker-compose -f ./docker-compose.yml down && docker-compose -f ./docker-compose.yml up -d",
    "logs_container": "docker logs -f as-template",
    "stop_container": "docker-compose -f ./docker-compose.yml down",
    "build_exe_container": "docker build -f ./dockerfile.exe -t as-template-exe .",
    "restart_exe_container": "docker-compose -f ./docker-compose.exe.yml down && docker-compose -f ./docker-compose.exe.yml up -d",
    "logs_exe_container": "docker logs -f as-template-exe",
    "stop_exe_container": "docker-compose -f ./docker-compose.exe.yml down"
  },
  "keywords": [],
  "author": "Alexey Sergeev <sega21rus98@mail.ru>",
  "license": "ISC"
}